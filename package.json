    "nodemon": "2.0.14",
    "sequelize-cli": "6.4.1",
    "ts-node": "^10.9.1",
    "typescript": "4.4",
    "@types/chai": "4.3.0",
    "@types/chai-http": "4.2.0",
    "@types/mocha": "9.1.0",
    "@types/sinon": "10.0.11",
    "@types/sinon-chai": "3.2.9",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "mocha": "9.2.1",
    "nyc": "15.1.0",
    "sinon": "13.0.1",
    "sinon-chai": "3.7.0"
  },
